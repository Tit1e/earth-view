{
  "manifest_version": 2,
  "name": "Earth View",
  "description": "Earth View",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_title": "Earth View"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "bookmarks"
  ],
  "icons": {
		"16": "images/logo.png",
		"48": "images/logo.png",
		"128": "images/logo.png"
	},
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}